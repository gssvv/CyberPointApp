    "colors": "^1.4.0",
    "cookie-parser": "^1.4.4",
    "cookieparser": "^0.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "dayjs": "^1.8.19",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "gsap": "^3.6.0",
    "joi": "^14.3.1",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.4.0",
    "luxon": "^1.11.2",
    "moment": "^2.24.0",
    "mongoose": "^5.4.12",
    "net": "^1.0.2",
    "node-sass": "^4.11.0",
    "node-schedule": "^1.3.2",
